{
  "name": "prop-trail",
  "publisher": "Ijemma",
  "repository": {
    "type": "git",
    "url": "https://github.com/ijemmao/prop-trail"
  },
  "galleryBanner": {
    "color": "#2B2B2B",
    "theme": "dark"
  },
  "icon": "logo.png",
  "displayName": "Prop Trail",
  "description": "Tracks all references of props within React components",
  "version": "1.1.3",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascriptreact",
    "onView:propTrailReferences"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.propTrail",
        "title": "PROP TRAIL"
      },
      {
        "command": "propTrail.jumpToReference",
        "title": "Edit Entry"
      },
      {
        "command": "propTrailReferences.reveal",
        "title": "Prop Trail: Reveal"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "propTrailReferences",
          "title": "Prop Trail References",
          "contextualTitle": "Prop Trial References",
          "icon": "resources/icon.svg",
          "visibility": "hidden"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "propTrailReferences",
        "contents": "No React prop trails found. Right-click on a React prop to see its trail."
      }
    ],
    "views": {
      "propTrailReferences": [
        {
          "id": "propTrailReferences",
          "name": "Prop Trail References"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "command": "extension.propTrail",
          "group": "navigation",
          "title": "PROP TRAIL"
        }
      ],
      "view/item/context": [
        {
          "command": "propTrail.jumpToReference",
          "when": "view == propTrailReferences"
        }
      ]
    },
    "languages": [
      {
        "id": "javascriptreact",
        "extensions": [
          ".js",
          ".jsx"
        ],
        "aliases": [
          "React",
          "js"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js",
    "deploy": "vsce package && vsce publish"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.38.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.2.0"
  },
  "dependencies": {
    "@babel/parser": "^7.14.2",
    "@babel/traverse": "^7.14.2",
    "@types/babel-types": "^7.0.9",
    "@types/babel__traverse": "^7.11.1",
    "@types/lodash": "^4.14.138",
    "babel-types": "^6.26.0",
    "lodash": "^4.17.21",
    "vsce": "^1.88.0"
  }
}
