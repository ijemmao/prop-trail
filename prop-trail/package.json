{
	"name": "prop-trail",
	"displayName": "prop-trail",
	"description": "track props in react components",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.propTrail"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.propTrail",
				"title": "PROP TRAIL"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.propTrail",
					"group": "navigation",
					"title": "PROP TRAIL"
				}
			]
		},
		"languages": [
			{
				"id": "javascriptreact",
				"extensions": [
					".js", ".jsx"
				],
				"aliases": [
					"React",
					"js"
				],
				"firstLine": "import React, { Component } from 'react';"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"@types/babel-traverse": "^6.25.5",
		"@types/babylon": "^6.16.5",
		"babel-traverse": "^6.26.0",
		"babylon": "^6.18.0"
	}
}
